<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.cs16jkd.myapplication.Operations.ImportToCalendar">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:textSize="30dp"
        android:padding="10dp"
        android:textColor="@android:color/black"
        android:layout_height="wrap_content"
        android:text="Import To Google Calendar" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="match_parent"
        android:layout_marginBottom="20dp"
        android:layout_height="wrap_content"
        android:text="Here is what was gathered from the timetable feel free to make the changes where you see fit." />

    <EditText
        android:id="@+id/captured_ocr_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textMultiLine" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Create Events" />
</LinearLayout>
